      <div class="footer__grp" data-aos="fade-up" data-aos-duration="1000">
        <div class="footer__logo"></div>
        <div class="footer__info_grp">
          <div class="footer__info footer__address">
            <div class="footer__icon">
              <i class="icon footer_address_icon"></i>
            </div>
